<% if !user_signed_in? %>
  <%= render "shared/navbar" %>
<% end %>
<div class="container-fluid" id="banner-landing">
  <% if !user_signed_in? %>
    <%= render "pages/cta" %>
  <% else %>
    <h1 class='text-center' style="padding-bottom: 30px; margin-top: 100px;">Find your App</h1>
  <% end %>
  <% @projects.each do |project| %>
    <div class="col-xs-12 col-md-6">
      <%= link_to project_path(project) do %>
        <%= render 'projects/card_link', project: project %>
      <% end %>
    </div>
  <% end %>
</div>
