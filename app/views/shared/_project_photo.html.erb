<% if project.project_assets.count != 0 %>
  <div class="card-img <%= classes %>" style="background-image: url('<%= project.main_asset.photo.identifier %>'); background-position: center; background-size: cover;">
  </div>
<% else %>
  <div class="card-img <%= classes %>"" style="background-image: url(<%= image_path "Hand_&_Logo.svg" %>); background-position: center;">
  </div>
<% end %>
